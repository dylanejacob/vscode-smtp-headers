{
    "name": "smtp-headers",
    "displayName": "SMTP Headers",
    "description": "Highlighting for SMTP Headers.",
    "version": "0.1.0",
    "publisher": "DylanJacob",
    "repository": "https://github.com/dylanejacob/vscode-smtp-headers",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "smtpheaders",
            "aliases": ["SMTP Headers", "smtpheaders"],
            "extensions": [".msg",".eml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "smtpheaders",
            "scopeName": "source.smtpheaders",
            "path": "./syntaxes/smtpheaders.tmLanguage.json"
        }]
    }
}